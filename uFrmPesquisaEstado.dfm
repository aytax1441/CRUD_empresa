  object QryBuscaEstado: TFDQuery
    Connection = frmPrincipal.ConnPrincipal
    SQL.Strings = (
      'SELECT '#9'c.IDUF,'
      #9#9'c.NMESTADO,'
      #9#9'c.SGESTADO,'
      #9#9'COUNT(DISTINCT c2.IDCIDADE) AS QTD_CIDADES'
      #9'FROM CADUF c '
      #9'LEFT JOIN CADCIDADE c2'
      #9'ON c2.IDUF = c.IDUF '
      '--<REF 01>'
      #9
      'GROUP BY c.IDUF, c.NMESTADO, c.SGESTADO')
    Left = 64
    Top = 153
  end
  object DsBuscaEstado: TDataSource
    DataSet = QryBuscaEstado
