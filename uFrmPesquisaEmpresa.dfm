  object QryBuscaEmpresa: TFDQuery
    Connection = frmPrincipal.ConnPrincipal
    SQL.Strings = (
      
        'SELECT empresa.IDEMPRESA, COUNT(DISTINCT cadfunc.IDFUNCIONARIO) ' +
        'QTDFUNC, empresa.NMEMPRESA, empresa.NUCNPJ, empresa.NUINSCRICAO ' +
        ', empresa.STATIVO'
      #9'FROM CADEMPRESA empresa'
      #9'LEFT JOIN CADFUNCIONARIOS cadfunc'
      #9'ON'
      #9#9'cadfunc.IDEMPRESA = empresa.IDEMPRESA'
      #9#9'AND cadfunc.STATIVO = '#39'S'#39
      #9#9'AND cadfunc.STEXCLUIDO = '#39'N'#39
      #9'LEFT JOIN CADTITULAR cadtit'
      #9'ON '
      #9#9'cadtit.IDEMPRESA = empresa.IDEMPRESA '
      #9#9'AND cadtit.STATIVO  = '#39'S'#39
      #9#9'AND CADFUNC.STEXCLUIDO = '#39'N'#39
      'WHERE empresa.STEXCLUIDO = '#39'N'#39
      '--<REF 01>'
      ''
      
        'GROUP BY empresa.IDEMPRESA, empresa.NMEMPRESA, empresa.NUCNPJ, e' +
        'mpresa.NUINSCRICAO, empresa.STATIVO')
    Left = 552
    Top = 128
  end
  object DsBuscaEmpresa: TDataSource
    DataSet = QryBuscaEmpresa
